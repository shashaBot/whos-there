{
  "name": "whos-there",
  "version": "1.0.0",
  "description": "A basic app setup to demonstrate the presence component that shows current users on a page.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/shashabot/whos-there.git"
  },
  "license": "MIT",
  "author": "Shashwat Gulyani",
  "scripts": {
    "start": "node ./bin/www",
    "debug": "DEBUG=whos-there:*,socket.io* nodemon --inspect ./bin/www",
    "postinstall": "npm rebuild node-sass",
    "test": "nyc mocha --timeout=10000 --exit",
    "lint": "eslint \"**/*.js\"",
    "validate": "npm-run-all --parallel test lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ladjs/bootstrap-social": "^7.0.2",
    "bcrypt": "^4.0.1",
    "bootstrap": "^4.4.1",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "~4.16.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.0",
    "jquery": "^3.5.0",
    "lusca": "^1.6.1",
    "mongoose": "^5.9.7",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-compass": "0.2.3",
    "node-sass": "^4.13.1",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "popper.js": "^1.16.1",
    "pug": "^2.0.4",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.3.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "mocha": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "supertest": "^5.0.0-0"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
